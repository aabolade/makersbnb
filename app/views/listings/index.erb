<!DOCTYPE html>

<html>
<body>
	<h1>Listings</h1>
	<form action="/listings/new" method="get">
		<input type="submit" value="create listing">
	</form>
	<ul id='listings'>
		<% @listings.each do |listing| %>
			<li>
				<a href='/listings/<%= listing.id %>'><%= listing.name %></a>
			</li>
		<% end %>
	</ul>

</body>
</html>
